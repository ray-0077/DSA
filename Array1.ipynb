    "import array as ar\n",
    "arr = ar.array('i',[2,7,5])\n",
    "l = list(arr)\n",
    "s = list()\n",
    "for i in range(len(l)):\n",
    "    for j in range(i,len(l)+1):\n",
    "        p = (l[i:j])\n",
    "        if p not in s and p!=[]:\n",
    "          s.append(p)\n",
    "\n",
    "print(*s)\n"
